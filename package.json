{
  "name": "bookmark-files",
  "displayName": "bookmark-files",
  "description": "it will bookmarks the files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bookmarkFiles",
          "title": "Bookmarks",
          "icon": "media/bookmark.svg"
        }
      ]
    },
    "views": {
      "bookmarkFiles": [
        {
          "id": "bookmarkExplorer",
          "name": "Bookmarks",
          "title": "Bookmarked Files"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "bookmark-files.removeBookmark",
          "when": "view == bookmarkExplorer",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "bookmark-files.addBookmark",
        "title": "Add to Bookmarks"
      },
      {
        "command": "bookmark-files.removeBookmark",
        "title": "Remove Bookmark"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}